<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.haruka.originalapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="Common" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DayActivity"></activity>

        <activity android:name=".TempActivity"></activity>

        <activity android:name=".ReminderActivity"></activity>

        <activity android:name=".DrumrollActivity"></activity>

        <activity android:name=".TeasingActivity"></activity>

        <activity android:name=".AnnounceActivity"></activity>

        <activity android:name=".EndActivity"></activity>

    </application>

</manifest>
